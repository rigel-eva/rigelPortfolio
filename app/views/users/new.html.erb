<div class="login">
  <div class="container">
    <div class="form">
        <h1>Sign up</h1>
        <%= form_for @user do |f|%>
            <%= f.label :username%>
            <%= f.text_field :username%>
            <%=f.label :email%>
            <%=f.email_field :email%>
            <%=f.label :password%>
            <%=f.password_field :password%>
            <%=f.submit%>
        <%end%>
    </div>
  </div>
</div>